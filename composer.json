{
  "name": "logbucket/laravel-logbucket",
  "description": "LogBucket для Laravel",
  "keywords": [
    "logbucket",
    "laravel-logbucket"
  ],
  "homepage": "https://github.com/logbucket/laravel-logbucket",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andrey Abramov",
      "email": "an.abramov@internet.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.6",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "LogBucket\\LaravelLogBucket\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LogBucket\\LaravelLogBucket\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelLogBucketServiceProvider"
      ],
      "aliases": {
        "LaravelLogbucket": "LaravelLogBucketFacade"
      }
    }
  }
}
